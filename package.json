    "axios": "^1.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.2.0",
    "google": "^2.1.0",
    "next": "^14.2.0",
    "next-pwa": "^5.6.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.8.0",
    "react-dom": "^18.3.0",
